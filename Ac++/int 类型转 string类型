#include<iostream>
#include<sstream>
using namespace std;
int main() {
  int n, a = 0;
  cin >> n;
  stringstream ss;
  ss << n;
  string s1 = ss.str();
  for (int i = 0; i < s1.length(); i++) {
    if (s1[i] != '\0') {
      switch (s1[i]) {
      case '0':
        a += 0;
     //   break;
      case '1':
        a += 1;
      //  break;
      case '2':
        a += 2;
     //   break;
      case '3':
        a += 2;
       // break;
      case '4':
        a += 3;
       // break;
      case '5':
        a += 3;
      //  break;
      case '6':
        a += 3;
     //   break;
      case '7':
        a += 3;
      //  break;
      case '8':
        a += 4;
        //break;
      case '9':
        a += 4;
      //  break;
      }
    }
  }
  cout << a;
  return 0;
}

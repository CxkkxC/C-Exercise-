#include<iostream>
using namespace std;
int main()
{

	int y[5][5]={1,2,3,4,5,4,6,7,8,9,2,5,7,6,9,7,6,8,5,6,2,3,4,7,5};
	int n,m,sum=0,i,j;
		for (n=0;n<5;n++)
	for (m=0;m<5;m++)
{
		if (n==m)
	sum+=y[n][m];
	}
	cout<<"主对角线的和为:"<<sum<<endl;
	
	sum=0;
		for(i=0;i<5;i++)
	for(j=0;j<5;j++)
	{
		if(j==4-i)
	sum+=y[i][j];
	}
	cout<<"次对角线的和为:"<<sum<<endl;
	return 0;
		}
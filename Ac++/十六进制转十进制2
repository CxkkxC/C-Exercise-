#include<iostream>
#include<string.h>
using namespace std;
long sum = 0;
long Hexadecimal(string s) {
  int i, a;
  for (i = 0; i < s.length(); i++) {
    if (s[i] <= '9')
      a = s[i] - '0';
    else
      a = s[i] - 'A' + 10;
    sum = sum * 16 + a;
  }
  return sum;
}
int main() {
  string hex;
  cin >> hex;
  sum = Hexadecimal(hex);
  cout << sum;
} 
#include<iostream>
using namespace std;
void array(int a[3][2])
{
int t,b[3][2];
for(int i=0;i<3;i++)
{
for(int j=0;j<2;j++)
{
	b[j][i]=a[i][j];
}
}
for(int p=0;p<2;p++)
{
for(int q=0;q<3;q++)
{
cout<<b[p][q]<<" ";
}
cout<<endl;
}
}
int main()
{
	int c[3][2];
	for(int n=0;n<3;n++)
{
for(int m=0;m<2;m++)
{
	cin>>c[n][m];
	}
	}
	array(c);
	return 0;
	}
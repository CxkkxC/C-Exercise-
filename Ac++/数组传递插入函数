#include <iostream>
using namespace std;
void Cxk(int a[],int L,int Sz)
{
	/*先插入数字，后冒泡法排序数组。*/
	int temp;
	a[L-1]=Sz;//最后一位插入数字
	for (int i=0;i<L-1;i++)
	for (int j=0;j<L-1-i;j++)
	{
		  if(a[j]>a[j+1]) //如果比较大的就替代身份，转到下一轮
  {
  	temp=a[j];
  a[j]=a[j+1];
  a[j+1]=temp;
  }
  }
	}
int main()
{
	int b[6]={2,4,6,7,9};
	int a;
	cin>>a;
	//需要插入数组做元素的数字。
	Cxk(b,6,a);
	  for (int i=0;i<6;i++)
  {
		cout<<b[i]<<" ";
			}
	return 0;
}
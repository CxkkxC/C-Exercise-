#include<iostream>
using namespace std;
int main() {
  int n, i, j;
  cin >> n;
  int a[35][35];
  for (i = 0; i < n; i++) {
    for (j = 0; j <= i; j++) {
      if (i == 0 || j == 0 || i == j) // 两边
        a[i][j] = 1;
      else
        a[i][j] = a[i - 1][j] + a[i - 1][j - 1];
    }
  }
  for (i = 0; i < n; i++) {
    for (j = 0; j <= i; j++) {
      cout << a[i][j] << " ";
    }
    cout << endl;
  }
  return 0;
}

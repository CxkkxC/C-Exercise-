#include<iostream>
using namespace std;
int a[20][20];
int main() {
  int n, i, j;
  while (cin >> n) {
    for (i = 1; i <= n; i++) {
      for (j = 1; j <= n; j++)
        cin >> a[i][j];
    }
    int s=0;
    for (i = 1; i <= n; i++) {
      for (j = 1; j <= n; j++) {
        s += a[j][i];
      }
      if (s > n / 2) {
        cout << i << ' ';
      }
    }
    cout << endl;
  }
  return 0;
}

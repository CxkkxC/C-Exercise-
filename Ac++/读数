#include<iostream>
#include<string>
using namespace std;
int main() {
  string a;
  string b[10] =
    { "ling", "yi", "er", "san", "si", "wu", "liu", "qi", "ba", "jiu" };
  string c[11] =
    { "", "", "shi", "bai", "qian", "wan", " shi", "bai", "qian", "yi",
"shi" };
  cin >> a;
  int l = a.length(), j;
  for (int i = 0; i < l; i++) {
    j = a[i] - '0';
    /* if(i>=2) cout <<carry[i+1]<<" "<<chinese[j[i]] << " "; else cout <<
       chinese[j[i]] << " "; } */
    if (j == 0) {
      if (i < l - 1)
        if (a[i + 1] != 48)
          cout<<b[j]<<" ";
    } else if ((l - i == 2 || l - i == 6 || l - i == 10) && j == 1)
      cout<<c[l - i]<<" ";
    else if (a[i - 1] == 48 && j == 1)
      cout<<b[j]<<" "<<c[l - i];
    else
     cout<<b[j]<<" "<<c[l - i]<<" ";
  }
  printf("\n");
  return 0;
}

#include<iostream>
using namespace std;
int main()
{
	int a[100];
	for(int i=0;i<=100;i++)
	{
		a[i]=i;
	}
	for(int j=0;j<=99;j++)
	{
		cout<<"a["<<j<<"]="<<a[j]<<" ";
		if((j+1)%10==0)
		{
	cout<<'\n';
	}
	}
		for(int j=99;j>=0;j--)
	{
		if(j%2==0)
		{
		cout<<"a["<<j<<"]="<<a[j]<<" ";
		}
		if((j+1)%10==0)
		{
	cout<<'\n';
	}
	}
	return 0;
}